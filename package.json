    "icon": "icon.png",
    "version": "1.0.0",
    "publisher": "HonzaKubita",
    "repository": {
        "type": "git",
        "url": "https://github.com/HonzaKubita/git-checkout-autostash"
    },
    "engines": {
        "vscode": "^1.91.0"
    },
    "categories": [
